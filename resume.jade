    :stylus
      bwidth = 720

      p, ul, li, h1, h2, h3
        margin-top 5px;

      #header
        background-color black
        color white
        padding 0 0 5px 10px

      ::selection, ::-moz-selection, ::-webkit-selection
        background #666
        color #fff

      html
        background-color #FFFFFFF

      serif()
        font-family Garamond, Georgia, New York Times, Times New Roman, serif

      body
        margin 10px auto
        font-size 12pt
        width bwidth

      .threecol
        float left
        width (bwidth / 3)

      .clear
        clear both

      .right
        float right
        margin-left 5px

      hr
        clear both

      h1
        font-size 2em
        serif()

      .subtitle
        margin 20px 10px 0 0
        color #999
        float right

      .details
        display inline
        color #666

      a
        color #39f
        text-decoration none

      h2
        font-size 1.5em
        margin-left 10px
        serif()

      .indent
        margin-left 30px

      li
        list-style-type square
        margin-left 1.5em

      h3
        margin-left 20px
        serif()

      .skill
        display inline
        padding-right 0.5em

